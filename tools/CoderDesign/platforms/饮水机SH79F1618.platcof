	<fixcontents>
	   <fixcontent>
<![CDATA[
/*---------------------------------------AD口-------------------------------- */
/*ad宏定义区*/
//p4.0
#define		AD0PIN			0							
#define		AD0_OUT			P4CR |= (0X01<<AD0PIN)
#define 	AD0_IN			P4CR &= ~(0X01<<AD0PIN)
#define		AD0_SET			P4 |= (0X01<<AD0PIN)
#define		AD0_CLR			P4 &= ~(0X01<<AD0PIN)
#define		AD0_PULL_ON		P4PCR |= (0X01<<AD0PIN)		
#define		AD0_PULL_OFF	P4PCR &= ~(0X01<<AD0PIN)	
#define 	AD0_BANK		BANK0
//p4.1
#define		AD1PIN			1							
#define		AD1_OUT			P4CR |= (0X01<<AD1PIN)
#define 	AD1_IN			P4CR &= ~(0X01<<AD1PIN)
#define		AD1_SET			P4 |= (0X01<<AD1PIN)
#define		AD1_CLR			P4 &= ~(0X01<<AD1PIN)
#define		AD1_PULL_ON		P4PCR |= (0X01<<AD1PIN)		
#define		AD1_PULL_OFF	P4PCR &= ~(0X01<<AD1PIN)	
#define 	AD1_BANK		BANK0	
//p4.2
#define		AD2PIN			2							
#define		AD2_OUT			P4CR |= (0X01<<AD2PIN)
#define 	AD2_IN			P4CR &= ~(0X01<<AD2PIN)
#define		AD2_SET			P4 |= (0X01<<AD2PIN)
#define		AD2_CLR			P4 &= ~(0X01<<AD2PIN)
#define		AD2_PULL_ON		P4PCR |= (0X01<<AD2PIN)		
#define		AD2_PULL_OFF	P4PCR &= ~(0X01<<AD2PIN)	
#define 	AD2_BANK		BANK0
//p4.3
#define		AD3PIN			3							
#define		AD3_OUT			P4CR |= (0X01<<AD3PIN)
#define 	AD3_IN			P4CR &= ~(0X01<<AD3PIN)
