static void AD_PinInit(uint8_t i)
{
  //byte  idata i;
	switch(i)
	{
		case 0: AD0_IN; AD0_PULL_OFF; break;
		case 1: AD1_IN; AD1_PULL_OFF; break;
		case 2: AD2_IN; AD2_PULL_OFF; break;
		case 3: AD3_IN; AD3_PULL_OFF; break;
		case 4: AD4_IN; AD4_PULL_OFF; break;
		case 5: AD5_IN; AD5_PULL_OFF; break;
		case 6: AD6_IN; AD6_PULL_OFF; break;
		case 7: AD7_IN; AD7_PULL_OFF; break;
		default:break;
	}	
	
	ADCH  |= (1 << i);			//设置端口为AD模拟输入
}

uint8_t  gAD_Driver(uint8_t yADChannel)
{
	uint8_t yADValue;
	
	AD_PinInit(yADChannel);		//对端口进行初始化
	ADCON = (yADChannel << 1);	//选择模拟输入通道
	AD_REF;				        //设置AD基准电压VREE[VDD]
	SET_AD_TIME;			    //设置AD采样时间	
	OPEN_AD_MODLE; 			    //打开AD模块
	START_AD_OPERATE;		    //开始AD采样						
	while(JUDGE_AD_OVER);		//等待AD采样完成	
	
	yADValue = AD_VALUE;	
	return yADValue;
}
